  "charts": [
    {
      "id": 1,
      "slice_name": "GDP by Industry Over Time",
      "viz_type": "line",
      "params": {
        "datasource": "gdp_summary__table",  
        "metrics": ["gdp_total"],
        "groupby": ["year"],
        "series_columns": ["industry"],
        "time_range": "No filter",
        "row_limit": 5000
      }
    },
    {
      "id": 2,
      "slice_name": "Top Industries by GDP",
      "viz_type": "bar",
      "params": {
        "datasource": "gdp_summary__table",
        "metrics": ["gdp_total"],
        "groupby": ["industry"],
        "order_desc": true,
        "row_limit": 10,
        "time_range": "No filter"
      }
    },
    {
      "id": 3,
      "slice_name": "GDP Growth (YoY)",
      "viz_type": "line",
      "params": {
        "datasource": "gdp_summary__table",
        "metrics": ["gdp_total"],
        "groupby": ["year"],
        "time_compare": ["1 year ago"],
        "row_limit": 5000
      }
    }
  ]
